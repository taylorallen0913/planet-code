{
  "name": "planet-code",
  "author": "Taylor Allen",
  "version": "1.0.0",
  "description": "This is the planet code website.",
  "main": "server.js",
  "scripts": {
    "install-all": "npm install --prefix client && npm install",
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon --exec babel-node server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "axios": "^0.19.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^5.2.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.17",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-router-dom": "^5.2.0",
    "stripe": "^8.60.0",
    "uuid": "^3.4.0",
    "validator": "^12.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "nodemon": "^2.0.4"
  }
}
